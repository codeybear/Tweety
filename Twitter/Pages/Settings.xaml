<Window x:Class="Pages.Settings"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Settings" Height="568" Width="825" Background="White" WindowStartupLocation="CenterScreen" FocusManager.FocusedElement="{Binding ElementName=txtUserName}" ResizeMode="NoResize">
    <Canvas Height="auto" Name="CanvasMain" Width="auto" Margin="0,0,0,-1">
        <Button Canvas.Left="680" Canvas.Top="507" Height="23" Name="btnOk" Width="62" Background="OrangeRed" Click="btnOk_Click" Content="Ok" TabIndex="2" IsDefault="True" HorizontalAlignment="Right" VerticalAlignment="Bottom"></Button>
        <Button Canvas.Left="747" Canvas.Top="507" Height="23" Name="btnCancel" Width="62" Background="LightSkyBlue" Click="btnCancel_Click" Content="Cancel" TabIndex="3" IsCancel="True" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="0"></Button>
        <WebBrowser x:Name="WebBrowser" Height="479" Canvas.Left="8" Canvas.Top="8" Width="803"/>
        <TextBox x:Name="txtUpdate" Canvas.Left="33.49" TextWrapping="Wrap" Canvas.Top="492" Width="95.553" Background="{x:Null}"/>
        <Label Content="PIN:" Canvas.Top="490.52" Canvas.Left="2"/>
    </Canvas>
</Window>
